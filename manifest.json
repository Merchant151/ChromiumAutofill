{
	"manifest_version":3,
	"name": "Chrome Autofill",
	"description": "A tool for automatic autofill",
	"version": "0.1.0",
	"action":{
	"default_popup": "index.html"},
	"permissions": [
		"storage",
		"offscreen"
	],
	"host_permissions":[
    		"https://*/*",
    		"http://*/*"
  	],
	"content_scripts": [
		{
			"matches":["https://talktalk.wd3.myworkdayjobs.com/TalkTalkCareers/*"],
			"matches":["https://talktalk.wd3.myworkdayjobs.com/*"],
			"js":["basic.js"]
		}
	]

}
